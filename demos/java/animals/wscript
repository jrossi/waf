#! /usr/bin/env python

def build(bld):

    bld(features  = 'javac seq',
        srcdir    = 'src',
        compat    = '1.5',
        name      = 'animals-src',
		seq_start = True
    )

    bld(features  = 'jar seq',
        basedir   = 'src',
        destfile  = 'animals.jar',
        #use       = 'animals-src',
        name      = 'animals'
    )

